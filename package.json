{
  "name": "amt-commander",
  "version": "0.2.1",
  "main": "main.js",
  "description": "AMT Commander is a standalone device management console for interacting with Intel Active Management Technology (AMT) devices over the local network",
  "author": {
    "name": "Matt C. Primrose"
  },
  "scripts": {
    "ng": "ng",
    "json-server": "node server.js --watch db.json",
    "ng-serve": "ng serve",
    "clean": "rimraf dist",
    "prestart": "npm run build",
    "start": "npm run electron",
    "electron": "electron .",
    "prebuild": "npm run clean",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "npm run build -- --configuration=production",
    "release": "electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.2",
    "@angular/cdk": "^16.0.1",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.0.1",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@open-amt-cloud-toolkit/wsman-messages": "^5.4.0",
    "crypto-browserify": "^3.12.0",
    "http-z": "^6.1.2",
    "json-server": "^0.17.3",
    "net": "^1.0.2",
    "path-browserify": "^1.0.1",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.6.1",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "~16.0.2",
    "@angular/compiler-cli": "^16.0.0",
    "@types/angular-material": "^1.1.73",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.2.5",
    "@types/uuid": "^9.0.1",
    "electron": "^24.3.1",
    "electron-builder": "^24.4.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^5.0.1",
    "typescript": "~5.0.2"
  }
}
